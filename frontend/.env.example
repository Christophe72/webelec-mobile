NEXT_PUBLIC_API_BASE="http://localhost:8080/api" # Dev backend (Spring Boot)
# NEXT_PUBLIC_API_BASE="https://api.webelec.be/api" # Prod backend URL
# Clé OpenAI (obligatoire pour les embeddings)
OPENAI_API_KEY="sk-xxxx"
# Optionnel : URL et modèle OpenAI si vous ne voulez pas ceux par défaut
# OPENAI_BASE_URL="https://api.openai.com"
# OPENAI_MODEL="text-embedding-3-large"
# Optionnel (démo locale uniquement): autoriser un fallback IA non-OpenAI
# WEBELEC_ALLOW_LOCAL_AI_FALLBACK="true"
# ID du vector store pour la recherche de fichiers RGIE (obligatoire pour les features IA)
# VECTOR_STORE_ID="vs_xxx"
# Clé JWT commune backend/frontend (obligatoire)
WEBELEC_JWT_SECRET="dev-webelec-secret-change-me-please-0123456789"
# Optionnel : paramètres d'issuer/audience si configurés côté backend
# JWT_ISSUER="webelec-backend"
# JWT_AUDIENCE="your-audience"

